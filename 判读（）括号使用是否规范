 #括号使用规范与否，在未遍历所有数据之前，"（"必须大于等于")"
 def f(list):
   ...:     s1 = 0
   ...:     s2 = 0
   ...:     for i in list:
   ...:         if i == '(':
   ...:             s1 =s1 + 1
   ...:         if i == ')':
   ...:             s2 = s2 + 1
   ...:         if s2 > s1:
   ...:             break
   ...:     return s1 == s2
